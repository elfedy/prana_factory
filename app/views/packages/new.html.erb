<h1>Nuevo Paquete</h1>

<%= form_for(@package, url: category_packages_path) do |f| %>
  <%= f.label :quantity, "Cantidad:" %>
  <%= f.text_field :quantity %>
  <%= f.label :unit, "Unidad de Medida:" %>
  <%= f.text_field :unit %>
  <%= f.label :normal_price, "Precio paquetes productos comunes:" %>
  <%= f.text_field :normal_price %>
  <%= f.label :premium_price, "Precio paquetes productos premium:" %>
  <%= f.text_field :premium_price %>
  <%= f.submit "Crear Paquete" %>
<% end %>
