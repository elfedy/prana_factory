<h1>Editar Producto</h1>

<%= form_for(@product, url: category_product_path(@category, @product)) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :name, "Nombre" %>
  <%= f.text_field :name %>
  <%= f.label :premium, "Producto Premium" %>
  <%= f.check_box :premium %>
  <%= f.submit "Editar Producto" %>
<% end %>
