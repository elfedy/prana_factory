<div class="row" id="header">
  <div class="col-xs-4 col-xs-offset-1">
    <%= link_to image_tag("PranaFactoryLogo-Header.png", :id => "logo"), root_path %>
  </div>

  <div class="col-xs-2">
    <% if admin_logged_in? %>
      <p>Administrador <%= current_admin.name %></p>
      <%= link_to "Inicio", admins_actions_path %>
      <%= link_to "Categorías", categories_path %>
      <%= link_to "Pedidos", checkouts_path %>
      <%= link_to "Cerrar Sesión", prana_admin_logout_path, method: :delete %>
    <% end %>
  </div>

  <div class="col-xs-3 col-xs-offset-1" id="sn-links">
    <%= link_to image_tag("FacebookLogo-Small.png"), "https://www.facebook.com/pranafactory/" %>
    <%= link_to image_tag("InstagramLogo-Small.png"), "https://www.instagram.com/pranafactory/" %>
  </div>
</div>

