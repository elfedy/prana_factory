<h1>Listado de SKU para la Categor√≠a <%= @category.name %></h1>
<div class="row">
  <div class="col-xs-3">
    <h2>Id</h2>
  </div>
  <div class="col-xs-3">
    <h2>Producto</h2>
  </div>
  <div class="col-xs-3">
    <h2>Paquete</h2>
  </div>
  <div class="col-xs-3">
    <h2>Precio</h2>
  </div>
</div>

<% @skus.each do |sku| %>
  <div class="row">
    <div class="col-xs-3">
      <%= sku.id %>
    </div>
    <div class="col-xs-3">
      <%= sku.product_name %>
    </div>
    <div class="col-xs-3">
      <%= sku.package_identifier %>
    </div>
    <div class="col-xs-3">
      <%= sku.price %>
    </div>
  </div>
<% end %>
